<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>
  <arg name="model" default="$(find obstacle_avoidance)/urdf/myrobo.xacro"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    
  </include>
    <param name="robot_description" command="cat '$(find obstacle_avoidance)/urdf/myrobo.xacro'" />

    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0.5"/>

    <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
          args="-urdf -param robot_description -model myrobo -x $(arg x) -y $(arg y) -z $(arg z)" />

</launch>